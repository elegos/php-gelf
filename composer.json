{
  "name": "giacomofurlan/php-graylog-gelf",
  "description": "A PHP PSR-3 compliant logger to send data to graylog2 (GELF format)",
  "type": "library",
  "keywords": ["graylog2", "gelf", "logger", "psr", "udp"],
  "license": "MIT",
  "authors": [
    {
      "name": "Giacomo Furlan",
      "email": "giacomo.furlan@fastwebnet.it",
      "homepage": "https://giacomofurlan.name",
      "role": "developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "GiacomoFurlan\\Graylog\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GiacomoFurlan\\Graylog\\Test\\": "test/"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^7.1",
    "psr/log": "^1.0",
    "ext-json": "*",
    "ext-sockets": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3",
    "php-mock/php-mock-phpunit": "^2.1"
  }
}
