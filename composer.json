{
  "name": "giacomofurlan/php-graylog2",
  "description": "A PHP PSR-3 compliant logger to send data to graylog2 (GELF format)",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Giacomo Furlan",
      "email": "giacomo.furlan@fastwebnet.it",
      "homepage": "https://giacomofurlan.name"
    }
  ],
  "autoload": {
    "psr-4": {
      "GiacomoFurlan\\Graylog\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GiacomoFurlan\\Graylog\\Test\\": "test/"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^7.1",
    "psr/log": "^1.0",
    "ext-json": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3"
  }
}
